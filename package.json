{
  "name": "rotten-node",
  "version": "0.1.7",
  "description": "A Node.js wrapper for Rotten Tomatoes API",
  "main": "lib/index.js",
  "scripts": {
    "preinstall": "npm i -g nodemon ava babel",
    "test": "nodemon --watch tests.js --exec ava tests.js",
    "dev": "nodemon --watch src --exec babel-node rollup.js",
    "build": "babel-node rollup.js"
  },
  "repository": "mr-wildcard/rotten-node",
  "keywords": [
    "Rotten",
    "Tomatoes",
    "API",
    "Node.js",
    "Javascript"
  ],
  "author": "Julien <dev.vialajulien@gmail.com> (https://github.com/mr-wildcard)",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.10.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.4.3",
    "rollup": "^0.25.1",
    "rollup-plugin-babel": "^2.3.9"
  },
  "dependencies": {
    "superagent": "^1.7.1"
  },
  "ava": {
    "require": "babel-register",
    "tap": true
  }
}
